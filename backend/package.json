{
  "name": "campusgrid-backend",
  "version": "1.0.0",
  "description": "CampusGrid Backend - College Internship Cell Exam Platform",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node tests/system.test.js",
    "test:load": "node tests/load.test.js",
    "seed": "node scripts/seedData.js",
    "create-admin": "node scripts/createAdmin.js",
    "create-indexes": "node scripts/createIndexes.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop campusgrid",
    "prod:restart": "pm2 restart campusgrid",
    "prod:logs": "pm2 logs campusgrid",
    "prod:monit": "pm2 monit"
  },
  "keywords": ["exam", "internship", "college", "mern"],
  "author": "Utsav Mistry",
  "contributors": ["Nishit Somani"],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "@sendinblue/client": "^3.3.1",
    "vm2": "^3.9.19",
    "cookie-parser": "^1.4.6",
    "redis": "^4.6.0",
    "bull": "^4.11.0",
    "compression": "^1.7.4",
    "cluster": "^0.7.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "axios": "^1.6.0"
  }
